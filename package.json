{
  "name": "pentacle",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:format:check": "./node_modules/.bin/nx format:write:check --base=origin/next --base=HEAD",
    "affected:format": "./node_modules/.bin/nx format:write --base=origin/next --base=HEAD",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "api:build:app": "tsc -p apps/api/tsconfig.app.json",
    "api:build:image": "docker build -f ./apps/api/Dockerfile -t pentacle-api:latest .",
    "api:publish": "docker save pentacle-api:latest | bzip2 | pv | ssh $PENTACLE_USER_NAME@$PENTACLE_DOMAIN 'bunzip2 | docker load'",
    "api:start": "API_BASE=http://cms.pentacledev.com/wp-json npm run start api",
    "web:start:local": "PROXY_TARGET=local npm run start web",
    "web:start:live": "PROXY_TARGET=live npm run start web",
    "build": "ng build",
    "cm": "git-cz",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "e2e": "ng e2e",
    "format:check": "./node_modules/.bin/nx format:check",
    "format:write": "./node_modules/.bin/nx format:write",
    "format": "./node_modules/.bin/nx format:write",
    "help": "./node_modules/.bin/nx help",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "update:check": "ng update",
    "update": "ng update @nrwl/schematics",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@clr/angular": "^1.0.3-patch",
    "@clr/icons": "^1.0.3-patch",
    "@clr/ui": "^1.0.3-patch",
    "@marblejs/core": "^1.2.1",
    "@marblejs/middleware-body": "^1.2.1",
    "@marblejs/middleware-joi": "^1.2.1",
    "@marblejs/middleware-logger": "^1.2.1",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@nrwl/nx": "7.0.2",
    "@webcomponents/custom-elements": "^1.0.0",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "core-js": "^2.5.4",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.11",
    "ngrx-store-localstorage": "^5.1.0",
    "prismjs": "^1.15.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.3.3",
    "rxjs-toolkit": "^1.1.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "7.0.1",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@ngrx/schematics": "^7.0.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/builders": "7.0.2",
    "@nrwl/schematics": "7.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.0.0",
    "@types/jquery": "3.3.6",
    "@types/node": "^8.9.5",
    "apidoc": "^0.17.7",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.0.5",
    "cypress": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.0.0",
    "jest-preset-angular": "6.0.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "pv": "^1.0.1",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
